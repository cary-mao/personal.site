{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/multi-word-component-names": "off"
    },
    "ignorePatterns": [
        "docs/.vuepress/**/config.js",
        "docs/.vuepress/theme/index.js",
        "!docs/.vuepress/theme/**/*.vue"
    ]
}